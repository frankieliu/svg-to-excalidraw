{
  "name": "svg-to-excalidraw",
  "version": "0.0.2",
  "description": "Convert SVG to Excalidraw's file format",
  "main": "dist/bundle.js",
  "bin": {
    "svg-to-excalidraw": "./bin/svg-to-excalidraw.js"
  },
  "files": [
    "dist/bundle.js",
    "bin/svg-to-excalidraw.js",
    "svg-to-excalidraw.d.ts"
  ],
  "types": "svg-to-excalidraw.d.ts",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider npm run clean && NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.js",
    "build:watch": "NODE_OPTIONS=--openssl-legacy-provider npm run clean && NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.js --watch",
    "clean": "rimraf dist",
    "fix": "npm run fix:other && npm run fix:code",
    "fix:code": "npm run test:code -- --fix",
    "fix:other": "npm run prettier -- --write",
    "lint:check": "eslint .",
    "lint:write": "eslint . --fix",
    "prettier": "prettier \"**/*.{css,scss,json,md,html,yml}\" --ignore-path=.eslintignore",
    "test": "npm run test:typecheck && npm run test:code && npm run test:other",
    "test:code": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test:other": "npm run prettier -- --list-different",
    "test:typecheck": "tsc"
  },
  "prettier": "@excalidraw/prettier-config",
  "contributors": [
    "Nicolas Goudry <goudry.nicolas@gmail.com>",
    "Broch Stilley <brochington@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.14.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-typescript": "7.15.0",
    "@excalidraw/eslint-config": "1.0.1",
    "@excalidraw/prettier-config": "1.0.2",
    "@types/chroma-js": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "babel-loader": "^8.2.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "typescript": "4.4.2",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "chroma-js": "^2.1.2",
    "commander": "^14.0.3",
    "dommatrix": "^0.1.1",
    "gl-matrix": "^3.3.0",
    "jsdom": "^28.0.0",
    "nanoid": "^3.1.25",
    "path-data-parser": "^0.1.0",
    "points-on-curve": "^0.2.0",
    "points-on-path": "^0.2.1",
    "roughjs": "^4.4.1"
  }
}
